<%- include("partials/header") %>

  <h1>Home</h1>
  <p>
    <%= value %>
  </p>

  <% posts.forEach(function(elem){ %>
    <h1>
      <%= elem.t %>
    </h1>
    <p>
      <%= elem.m.substring(0, 100) + " ..." %>
      <a href="/posts/<%= elem.t %> ">Read more</a>
    </p>

    <% }) %>

      <%- include("partials/footer") %>